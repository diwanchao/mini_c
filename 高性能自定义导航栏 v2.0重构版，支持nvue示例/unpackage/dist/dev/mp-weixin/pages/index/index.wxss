@font-face {
  font-family: 'hxicon';
  /* project id 2009392 */
  src: url(data:font/truetype;charset=utf-8;base64,) format("truetype");
}
.hxicon {
  font-family: hxcion;
  text-decoration: none;
}
.foot {
  height: 40px;
}
.tit {
  font-size: 18px;
}
.list {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  background-color: #fff;
  margin-top: 10px;
}
.list-head {
  position: relative;
  background-color: #f8f8f8;
  padding: 10px 22px;
  color: #333;
  border-style: solid;
  border-color: #E3E3E3;
  border-bottom-width: 1px;
}
.list-head-before {
  position: absolute;
  left: 13px;
  top: 10px;
  bottom: 10px;
  width: 3px;
  background-color: #d8d8d8;
  -webkit-border-radius: 8px;
          border-radius: 8px;
}
.list-item {
  position: relative;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  margin: 0 0 0 13px;
  padding: 10px 60px 10px 0;
  border-style: solid;
  border-color: #E3E3E3;
  border-bottom-width: 1px;
}
.list-item:last-child {
  border-bottom-width: 0;
}
.title {
  font-size: 14px;
  margin-bottom: 4px;
  color: #333333;
}
.note {
  font-size: 12px;
  color: #999999;
}
.icon_ctn {
  position: absolute;
  justify-items: center;
  right: 13px;
  top: 0;
  bottom: 0;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
}
.hxicon {
  color: #d8d8d8;
}
.ctn {
  background-color: #F1F1F1;
}

